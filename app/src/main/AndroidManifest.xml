    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>
    <uses-permission android:name="android.permission.VIBRATE" />

    <application
            android:name=".App"
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".brvah.SelectedActivity">
        </activity>
        <activity android:name=".brvah.ChoiceActivity">
        </activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".rxjava.RxJavaActivity">
        </activity>
        <activity android:name=".brvah.BrvahActivity">
        </activity>
        <activity android:name=".cache.CacheActivity">
        </activity>
        <activity android:name=".brvah.CheckBoxActivity">
        </activity>
        <activity android:name=".pipeta.PipetaActivity">
        </activity>
        <activity android:name=".pipeta.MoveActivity">
        </activity>
        <activity android:name=".dialog.DialogActivity"/>
        <activity android:name=".brvah.DimmerActivity">
        </activity>
        <activity android:name=".brvah.DimmerEditActivity">
        </activity>
        <activity android:name=".pipeta.SeekBarActivity">
        </activity>
        <activity android:name=".viewpager.ViewPagerActivity">
        </activity>
        <activity
                android:name=".immersion.ImmersionTypeActivity"
                android:theme="@style/NoActionBarTheme"/>
        <activity android:name=".immersion.ImmersionActivity">
        </activity>
        <activity
                android:name=".immersion.ImmersionMallActivity"
                android:label="@string/title_activity_immersion_mall"
                android:theme="@style/NoActionBarTheme">
        </activity>
        <activity
                android:name=".netease.NeteaseActivity"
                android:theme="@style/NoActionBarTheme">
        </activity>

        <service
                android:name=".netease.service.MusicService"
                android:enabled="true"
                android:exported="false"/>

        <activity android:name=".netease.Lock9ViewActivity">
        </activity>
    </application>

</manifest>